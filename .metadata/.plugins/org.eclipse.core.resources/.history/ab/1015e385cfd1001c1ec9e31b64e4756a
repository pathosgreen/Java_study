package Assignment4;

import java.util.Scanner;

public class Assignment4_05 {
	public static void main(String[] args) {
		//1부터 n까지 합 구하기
		
		Scanner scanner = new Scanner(System.in);
		
		System.out.println("1부터 n까지 합 구하기");
		int n;
		do {
			System.out.print("양의 정수 : ");
			n = scanner.nextInt();
		}while(n<=0);
		
	}
}
