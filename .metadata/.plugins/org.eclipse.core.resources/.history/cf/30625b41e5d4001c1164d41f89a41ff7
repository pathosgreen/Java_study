package test;

public class Test04 {
	public static void main(String[] args) {
		// 등수 구하기
		
		int[] point = {98,70,45,60,83};
		int[] rank;
		int temp=0;
		for(int i=0;i<point.length; i++) {
			for(int j=1;j<point.length; j++ ) {
				if( point[i]<point[j]) {
					System.out.println(point[i]"<"point[j]);
				}
			}
		}
		
	}
}
