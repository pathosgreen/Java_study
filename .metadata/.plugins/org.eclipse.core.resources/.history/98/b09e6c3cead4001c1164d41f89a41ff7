package test;

public class Test05 {
	public static void main(String[] args) {
		// 조합
		
		String[] name = {"aaa","bbb","ccc","ddd","eee"};
		int[] point = {98,70,45,60,83};
		int[] rank = {5,4,3,2,1};
		
		
		
		
		
		
		
		
		
		
		for(int i=0;i<rank.length; i++) {
			System.out.println(i+1 +"번 "+name[i]+" "+ point[i]+"점 "+rank[i]+"등");
		}
	}
}
