package Assignment3;

import java.util.Scanner;

public class Assignment3_05 {
	public static void main(String[] args) {
		//양의 정숫값을 0까지 카운트다운
		Scanner scanner = new Scanner(System.in);
		System.out.print("양수 a : "); int a = scanner.nextInt();
		
		if(a <= 0) {
			System.out.println("a가 0이하임");
		}else {
			for(int i=a; i<0; i--) {
				System.out.print(i + " ");
			}
		}
		
	}
}
