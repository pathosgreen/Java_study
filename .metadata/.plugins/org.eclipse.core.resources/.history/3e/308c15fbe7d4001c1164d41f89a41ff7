package test;

public class Test04 {
	public static void main(String[] args) {
		// 등수 구하기
		
		int[] point = {98,70,45,60,83};
		int[] rank = {5,4,3,2,1};
		int temp=0;
		for(int i=0;i<point.length; i++) {
			for(int j=i+1;j<point.length; j++ ) {
				if(rank[i]<rank[j]) {
					if(point[i]<point[j]) {
						temp = rank[i];
						rank[i] = rank[j];
						rank[j] = temp;						
					}else {
					if(point[i]>point[j]) {
						temp = rank[i];
						rank[i] = rank[j];
						rank[j] = temp;	
						}
					}
				}
			}
		}
		for(int i=0;i<rank.length; i++) {
			System.out.println(i+1 +"번"+ point[i]+"점"+rank[i]+"등");
		}
	}
}
